# Multi-Agent Claude Code Environment Variables
# Copy this to .env and fill with your actual values
# DO NOT COMMIT .env FILE

# Core Services
# ===================================

# Anthropic (Claude) - Required for Claude Code
ANTHROPIC_API_KEY="sk-ant-api03-YOUR-KEY-HERE"

# GitHub Token - Required for GitHub operations
GITHUB_TOKEN="ghp_YOUR-TOKEN-HERE"

# Optional Services
# ===================================

# Supabase (if using database)
SUPABASE_URL="https://YOUR-PROJECT.supabase.co"
SUPABASE_ANON_KEY="YOUR-ANON-KEY"
SUPABASE_SERVICE_KEY="YOUR-SERVICE-KEY"

# OpenAI (if using GPT models)
OPENAI_API_KEY="sk-YOUR-KEY-HERE"

# Vercel (if deploying to Vercel)
VERCEL_TOKEN="YOUR-VERCEL-TOKEN"

# Add other service keys as needed for your project